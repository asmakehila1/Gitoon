<form method="post" action="{{ path('temoin_delete', {'id': temoin.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input  type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ temoin.id) }}">
    <button class="btn btn-inverse waves-effect waves-light">Delete</button>
</form>
